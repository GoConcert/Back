<%- model_class = MusicStyle -%>
<div class="page-header">
  <h1>Concert-preferences search</h1>
</div>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>
          <%= model_class.human_attribute_name(:id) %>
        </th>
        <th>
          <%= model_class.human_attribute_name(:concert_id) %>
        </th>
		        <th>
          <%= model_class.human_attribute_name(:style_id) %>
        </th>
		        <th>
          <%= model_class.human_attribute_name(:order) %>
        </th>
        <th>
          <%= model_class.human_attribute_name(:created_at) %>
        </th>
        <th>
          <%=t '.actions', :default => t("helpers.actions") %>
        </th>
      </tr>
    </thead>
    <tbody>
      <% @concert_preferences.each do |concert_preference| %>
      <tr>
        <td>
          <%= link_to concert_preference.id, concert_preference_path(concert_preference) %>
        </td>
        <td>
          <%= concert_preference.id %>
        </td>
        <td>
          <%= concert_preference.concert_id %>
        </td>
        <td>
          <%= concert_preference.style_id %>
        </td>
        <td>
          <%= concert_preference.order %>
        </td>
        <td>
          <%=l concert.created_at %>
        </td>
      </tr>
      <% end %>
    </tbody>
  </table>


